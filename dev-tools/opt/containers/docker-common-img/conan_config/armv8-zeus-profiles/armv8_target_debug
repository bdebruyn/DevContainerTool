toolchain=/usr/bin/aarch64-poky-linux
target_host=aarch64-poky-linux
cc_compiler=gcc
cxx_compiler=g++

[options]
*:sdk=True

[env]
CONAN_CMAKE_FIND_ROOT_PATH=$toolchain
CHOST=$target_host
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
CC=$target_host-$cc_compiler
CXX=$target_host-$cxx_compiler
STRIP=$target_host-strip
RC=$target_host-windres

[settings]
os_build=Linux
arch_build=x86_64
os=Linux
arch=armv8
compiler=gcc
compiler.cppstd=17
compiler.version=9.2
compiler.libcxx=libstdc++11
build_type=Debug

